            <string>///Create Datastructures!

//amount of something~
amount_of_liquids = 3;
amount_of_factions = 3;


//  LIQUIDS///////////////////////////////
global.liquids = ds_list_create();
//properties
var name, hue;
for (i=0; i&lt;amount_of_liquids; i+=1)
{
    global.liquids[| i] = ds_map_create();
    name = scr_name_liquid();
    hue = random_range(130,190);
    show_debug_message(string(name)); //shows all liquids
    
    ds_map_add( global.liquids[| i], "name", name );
    ds_map_add( global.liquids[| i], "hue", hue );
};

//  FACTIONS/////////////////////////////
global.factions = ds_list_create();
//properties
var name, legs, mass, spd, color;
for (i=0; i&lt;amount_of_factions; i+=1)
{
    global.factions[| i] = ds_map_create();
    name = string(totro(3,6,1));
    legs =  irandom(3)+irandom(3);
    mass = random_range(1,90) + random_range(1,90);
    spd = max(legs + 6 - mass/30, 0.2);
    color = make_color_hsv( random(255), 200 + random(55), 200 + random(55) );
    
    
    ds_map_add( global.factions[| i], "name", name );
    ds_map_add( global.factions[| i], "legs", legs );
    ds_map_add( global.factions[| i], "mass", mass );
    ds_map_add( global.factions[| i], "speed", spd );
    ds_map_add( global.factions[| i], "color", color );
};
</string>
